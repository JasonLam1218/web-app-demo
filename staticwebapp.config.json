{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'",
    "x-content-type-options": "nosniff",
    "x-frame-options": "DENY"
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
